{
  "name": "fetch-ui",
  "version": "1.0.0",
  "description": "Fetch UI CLI Tool",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fetch-ui": "./bin/fetch-ui.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@fetch-ui/core": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.2.0",
    "ora": "^7.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}